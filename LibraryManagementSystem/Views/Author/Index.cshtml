@model IEnumerable<Author>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<a class="btn btn-success mt-4" style="margin: 10px" asp-controller="Author" asp-action="AddAuthor">Create new author</a>
<div class="row ">
    @foreach (var author in Model)
    {
        @Html.Partial("_AuthorView", author)
    }
</div>